            "in": "query",
            "description": "Data de fim do relatório.",
            "schema": {
              "format": "date-time",
              "type": "string"
            }
          },
          {
            "name": "doctorId",
            "required": false,
            "in": "query",
            "description": "ID do médico para filtrar o relatório (opcional).",
