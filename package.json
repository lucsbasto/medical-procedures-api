  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "biome check",
    "format": "biome check --write",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- migration:generate src/infrastructure/database/typeorm/migrations/MakeContactOptional -d src/infrastructure/database/typeorm/config/postgres.provider.ts",
    "migration:run": "npm run typeorm -- migration:run -d src/infrastructure/database/typeorm/config/postgres.provider.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d src/infrastructure/database/typeorm/config/postgres.provider.ts"
  },
